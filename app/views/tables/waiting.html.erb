<% game ||= @game %>

<%= content_tag :div, id: "game-#{game.id}", class: "current-game", data: { game_id: game.id, table_id: game.table_id } do %>
  <div class="cat">
    <h5>
      Please enjoy this sweet cat for the time being.
    </h5>
    <br><br><br>
    <img src="http://placekitten.com/g/400/400">
  </div>
<% end %>
